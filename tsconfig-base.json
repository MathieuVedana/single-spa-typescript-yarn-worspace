{
  "$schema": "https://json.schemastore.org/tsconfig",
  "extends": "ts-config-single-spa",
  "compilerOptions": {
    "lib": ["dom", "dom.iterable", "ESNext"],
    "module": "node16",
    "target": "ES2022",
    "declaration": true,
    "declarationMap": true,
    "emitDeclarationOnly": true,
    "strict": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true
  }
}
